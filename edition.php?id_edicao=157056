<script language="javascript" type="text/javascript">
				function tabctrl() {
				    var x = document.getElementsByClassName("hd_tab_button");
					var i;
					var ha_tabs = 0
					for (i = 0; i < x.length; i++) {
					    if(x[i].style.display == 'block') ha_tabs = 1;
					}

